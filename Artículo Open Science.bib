@article{alanocaImpactoTransferenciaMonetaria2017,
	title = {Impacto de la transferencia monetaria condicionada del programa {JUNTOS} sobre el ingreso del hogar beneficiario en el Perú, 2013 – 2015},
	rights = {info:eu-repo/semantics/{openAccess}},
	url = {http://repositorio.unap.edu.pe/handle/UNAP/4902},
	abstract = {Los programas de transferencia monetaria condicionada tienen como propósito aliviar los  efectos de la pobreza a través de un subsidio directo al hogar beneficiario con el  cumplimiento de corresponsabilidades destinadas a mejorar el capital humano, si bien el  ingreso adicional debe aumentar el ingreso que dispone el hogar beneficiario, este puede  ser retraído por diversos factores no esperados. En el presente estudio de investigación se  examina el programa {JUNTOS} y se plantea como objetivo evaluar la existencia de algún  efecto sobre la percepción de ingresos del hogar beneficiario, principalmente su  incidencia sobre el ingreso laboral, también se determina el impacto del programa en loa  distintos puntos de la distribución del ingreso total de los hogares beneficiarios, utilizando  técnicas para estudios con diseño cuasiexperimental. Para la evaluación del programa  {JUNTOS}, se utiliza la base de datos panel de la {ENAHO} entre el periodo 2013 y 2015, a  partir de los cuales se conforman los grupos de tratamiento y control para luego estimar  el impacto del programa sobre la variable de resultado, aplicando los métodos de {PSM} y  dobles diferencias; encontrándose que el impacto del programa {JUNTOS} es positivo  sobre el ingreso total del hogar, y diferenciado para los hogares beneficiarios con los  ingresos más bajos pertenecientes al primer cuartil, también se evidencia un impacto  negativo sobre el ingreso laboral del hogar, pero no significativo. Los resultados  encontrados van en concordancia a la evidencia nacional e internacional de evaluación de  impacto de programas de transferencia monetaria condicionada},
	journaltitle = {Universidad Nacional del Altiplano},
	author = {Alanoca, Velasquez and Miguel, Yosi},
	urldate = {2021-05-29},
	date = {2017-07-04},
	note = {Accepted: 2017-08-17T15:28:13Z
Publisher: Universidad Nacional del Altiplano},
	file = {Full Text PDF:C\:\\Users\\monic\\Zotero\\storage\\QBXJXGFB\\Alanoca y Miguel - 2017 - Impacto de la transferencia monetaria condicionada.pdf:application/pdf;Snapshot:C\:\\Users\\monic\\Zotero\\storage\\XQUNT96S\\4902.html:text/html},
}

@article{landivarTransferenciasMonetariasCondicionadas2019,
	title = {Transferencias monetarias condicionadas y su incidencia sobre los indices de pobreza en Ecuador durante el período 2013-2017},
	rights = {{openAccess}},
	url = {http://repositorio.ug.edu.ec/handle/redug/41365},
	abstract = {Los programas de transferencia monetaria condicionados son instrumentos útiles para el alivio de la pobreza en muchos países.  Este trabajo se centra en el análisis de este instrumento, su impacto en el índice de pobreza en Ecuador, y el comportamiento de ambos en los períodos 2013 al 2017. Para el estudio se utilizaron los datos de la {CEPAL} para obtener los valores destinados a las transferencias monetarias condicionadas, y del {INEC} se obtuvieron los índices de pobreza del lustro a investigar. Para el cumplimiento del objetivo del estudio se determinó que El presupuesto de las transferencias monetarias condicionadas ha disminuido en un promedio de 11,99\% durante el período de estudio. Así mismo, se ha observado que tanto los niveles de pobreza como el de acuerdo con la pobreza urbana y rural han presentado una tendencia decreciente. Se determinó la relación existente entre las transferencias monetarias condicionadas y los índices de pobreza en el período de investigación. Utilizando el coeficiente de correlación de Pearson se obtuvieron valores positivos. La variable gasto social como proxy para identificar los niveles bajos de pobreza y se concluye que el coeficiente de correlación se convierte en negativo, indicando que a medida que aumente el gasto social los índices de pobreza disminuyen},
	author = {Landívar, Soto and Arturo, Oliver},
	urldate = {2021-05-29},
	date = {2019-04-20},
	note = {Accepted: 2019-07-04T20:57:56Z
Publisher: Universidad de Guayaquil. Facultad de Ciencias Económicas},
	file = {Full Text PDF:C\:\\Users\\monic\\Zotero\\storage\\QBKZJMLV\\Landívar y Arturo - 2019 - Transferencias monetarias condicionadas y su incid.pdf:application/pdf;Snapshot:C\:\\Users\\monic\\Zotero\\storage\\2SG6XBS4\\41365.html:text/html},
}

