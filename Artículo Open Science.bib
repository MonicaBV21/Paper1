
@article{castanedaTransferenciasCondicionadasMortalidad2020,
	title = {Transferencias condicionadas y mortalidad infantil municipal, el caso de Familias en Acción en Colombia},
	rights = {Atribución-{NoComercial}-{SinDerivadas} 2.5 Colombia},
	url = {http://repository.javeriana.edu.co/handle/10554/47588},
	abstract = {Los programas de transferencias monetarias condicionadas buscan combatir la pobreza de la población más vulnerable por medio de la construcción de capital humano, en el caso de Colombia, se ha implementado Familias en Acción. La evidencia muestra que este tipo de programas ha tenido efectos positivos sobre el número de controles de salud preventivos, tasas de matrícula escolar y nutrición. A pesar de los efectos positivos encontrados en cuanto a disminución de los índices de bajo peso al nacer y desnutrición, junto con mejores resultados en la asistencia y controles de salud, no existe evidencia acerca del impacto del programa sobre la mortalidad infantil. Este trabajo estima el impacto de una mayor exposición a Familias en Acción sobre la tasa de mortalidad infantil, para esto explotamos la intensidad en la que los municipios rurales han recibido el programa a través de una estrategia de diferencias en diferencias. Los resultados sugieren que a pesar de que la tasa de mortalidad infantil disminuyó más en los municipios expuestos por más tiempo al programa, la diferencia no es estadísticamente significativa.},
	author = {Castañeda, Riaño and Augusto, Cesar},
	urldate = {2021-05-29},
	date = {2020-03-10},
	note = {Accepted: 2020-03-10T21:28:36Z
Publisher: Pontificia Universidad Javeriana},
	file = {Full Text PDF:C\:\\Users\\monic\\Zotero\\storage\\9KZZJ6JP\\Castañeda y Augusto - 2020 - Transferencias condicionadas y mortalidad infantil.pdf:application/pdf;Snapshot:C\:\\Users\\monic\\Zotero\\storage\\9I42BRCH\\47588.html:text/html},
}

@article{bonillaAnalisisComparativoProgramas2020,
	title = {Análisis comparativo de los programas de transferencias monetarias condicionadas en Ecuador “Bono de desarrollo humano” y en Colombia “Más familias en acción” durante el período 2013 – 2017},
	url = {http://repositorio.puce.edu.ec:80/xmlui/handle/22000/18018},
	abstract = {El presente estudio es un análisis comparativo de los Programas de Transferencias Monetarias Condicionadas ({PTMC}) en Ecuador y Colombia. Para realizar dicho análisis, se aplicó la teoría de Desarrollo Humano de Amartya Sen con la finalidad de comparar la implementación del programa “Bono de Desarrollo Humano” ({BDH}) en Ecuador y “Más Familias en Acción” ({MFA}) en Colombia, para así determinar si dichos programas han contribuido a la promoción del desarrollo humano en ambos países mediante la creación de libertades instrumentales y la expansión de libertades constitutivas para los niños y jóvenes de las familias beneficiarias del {BDH} y de {MFA} durante el período 2013 – 2017. En ese sentido, esta disertación consta de tres capítulos. El primero constituye el capítulo teórico, en el cual se explicará las nociones de desarrollo humano de Amartya Sen, la importancia que esta perspectiva ha alcanzado en la agenda internacional y su relación con los {PTMC}. En el segundo capítulo se realiza un análisis de los fundamentos de los {PTMC} en América Latina, haciendo énfasis en el funcionamiento del {BDH} en Ecuador y de {MFA} en Colombia. El tercer capítulo es el metodológico, es decir, se realiza una comparación de las debilidades funcionales, así como de los resultados cuantitativos y cualitativos obtenidos por el {BDH} en Ecuador y por {MFA} en Colombia durante el período de análisis.},
	author = {Bonilla, Calero and Isabel, Emily},
	urldate = {2021-05-29},
	date = {2020-07-07},
	langid = {spanish},
	note = {Accepted: 2020-09-08T23:48:50Z
Publisher: {PUCE} - Quito},
	file = {Snapshot:C\:\\Users\\monic\\Zotero\\storage\\DLPQT69R\\18018.html:text/html},
}

